batch_size: 32

checkpoint_min: 0.5

# Directory of input folder with (TF_records)
input_path: '/kw_resources/data/coor_new_data_14aug'

# Directory of output folder (model to save)
result_path: '/kw_resources/result/debug_sharpness'

# Training steps
steps: 100000

learning_rate: 0.0001
# learning_rate: 0.000007

dropout_rate: 0.2

# Activation function: '0' is relu, '1' is leaky relu
activation: '1'

# Number of channels in CNN layer
channels: 3

# Comment to write in config file
comment : "New data try with large batch suze

# Run options: ["single", "kfold", "search", "kfold_search"]
run_mode: "search"

# Score type options : ["Occ_B_median", "Occ_F_median", "Occ_L_median", "BL_median", "MD_median", "Integrity_median", "Width_median", "Surface_median", "Sharpness_median"]
label_type: 'Sharpness'

# If true file, will be in folder of current time
use_current_time: false